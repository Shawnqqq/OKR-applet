.container{
  padding: 30rpx;
}
.title{
  font-size: 22rpx;
  color: #666;
  line-height: 2;
}
.OKR-item{
  display: flex;
  flex-direction: column;
  margin: 20rpx 0;
}
.objective{
  font-size: 28rpx;
  color: brown;
}
.keyresult{
  width: 100%;
  height: 60rpx;
  border: 1rpx solid #555;
  margin: 10rpx 0;
  font-size: 26rpx;
  color: #000;
  line-height: 60rpx;
  text-indent: 10rpx;
  letter-spacing: 1rpx;
}

.keyresult.active{
  background: rgb(60, 104, 226);
  color: #fff;
}
.tips{
  font-size: 30rpx;
  color: #777;
  text-align: center;
  margin-top: 400rpx;
}