.container{
  padding: 25rpx;
}
.objective-item{
  width: 100%;
  height: 150rpx;
  margin: 10rpx auto;
  border-radius: 8rpx;
  background: linear-gradient(to bottom right,rgb(76, 161, 247),rgb(208, 230, 252));
  padding: 20rpx;
  box-sizing: border-box;
  position: relative;
  overflow: hidden;
}
.objective-title{
  font-size: 32rpx;
  color: #111;
}
.objective-time{
  font-size: 20rpx;
  color: #555;
  text-indent: 15rpx;
}
.icon{
  position: absolute;
  top: -25rpx;
  right: -25rpx;
  width: 150rpx;
  height: 150rpx;
  font-size: 40rpx;
  color: #666;
  text-align: center;
  line-height: 150rpx;
  border-radius: 50%;
  border: 5rpx solid #666;
  transform: rotate(-45deg)
}
.keyresult-item{
  width: 100%;
  margin: 20rpx auto;
  padding: 30rpx;
  box-sizing: border-box;
  background-color: rgba(76, 0, 130, 0.05);
  border-radius: 15rpx;
}
.keyresult-content{
  display: flex;
  height: 40rpx;
  align-items: center;
}
.kr-dot{
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  background-color: #777;
  margin-right: 30rpx;
}
.kr-dot.active{
  background-color: #DC143C;
}
.kr-title{
  font-size: 30rpx;
  color: #555;
  flex: 1;
}
.kr-complete{
  font-size: 24rpx;
  color: #555;
}
.todo-list{
  margin: 20rpx 0 0 40rpx;
}
.todo-item{
  font-size: 24rpx;
  color: #666;
  line-height: 1;
}
.todo-item.active{
  text-decoration:line-through;
}